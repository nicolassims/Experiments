<div>
    <p>Authorized for: <%= get_user_name(@access) %></p>

    <p>Latest Tweet: <%= get_status(@access) %>
</div>

<div>
    <%= render(
        TwitterWeb.TweetView,
        "new.html",
        conn: @conn,
        action: Routes.tweet_path(@conn, :create, %{access: @access}),
    ) %>
</div>
